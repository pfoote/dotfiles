{
  "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.renderWhitespace": "all",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.matchBrackets": "never",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "workbench.sideBar.location": "right",
  "workbench.iconTheme": "material-icon-theme",
  "dateTime.padHours": false,
  "dateTime.showAMPM": true,
  "dateTime.showSeconds": true,
  "dateTime.showMonth": true,
  "dateTime.showDayOfMonth": true,
  "editor.minimap.enabled": false,
  "workbench.colorTheme": "Bartleby at Night",
  "bracket-pair-colorizer-2.colors": [ "Lime", "Magenta", "Cyan", "Yellow" ],
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "bracket-pair-colorizer-2.unmatchedScopeColor": "Red",
  "bracket-pair-colorizer-2.colorMode": "Consecutive",
  "bracket-pair-colorizer-2.activeScopeCSS": [
    "borderStyle : solid",
    "borderWidth : 1px",
    "borderColor : {color}",
    "opacity: 0.5",
  ],
  "bracket-pair-colorizer-2.rulerPosition": "Left",
  "bracket-pair-colorizer-2.forceIterationColorCycle": true,
  "bracket-pair-colorizer-2.scopeLineCSS": [
    "borderStyle : solid",
    "borderWidth : 1px",
    "borderColor : {color}",
    "opacity: 0.7"
  ],
  "bracket-pair-colorizer-2.showBracketsInRuler": true,
  "bracket-pair-colorizer-2.scopeLineRelativePosition": false,
  "bracket-pair-colorizer-2.forceUniqueOpeningColor": true,
  "spotify.priorityBase": -11,
  "spotify.trackInfoClickBehaviour" : "play_pause",
  "spotify.showSignInButton" : false,
  "spotify.showSignOutButton" : false,
  "spotify.showMuteUnmuteVolumeButton" : false,
  "spotify.showPlayPauseButton": false,
  "spotify.showLyricsButton" : false,
  "spotify.trackInfoPriority": 9,
  "typescript.suggest.paths": false,
  "editor.codeLens" : false,
  "powermode.enabled": true,
  "powermode.presets": "flames",
  "powermode.enableShake": false,
  "powermode.explosionDuration": 750,
  "powermode.explosionSize": 8,
  "powermode.gifMode": "continue",
  "powermode.explosionFrequency": 1,
  "powermode.enableStatusBarComboCounter": false,
  "powermode.enableStatusBarComboTimer": false,
  "powermode.backgroundMode": "mask",
  "errorLens.margin": "8ch",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "yaml.validate": false
}